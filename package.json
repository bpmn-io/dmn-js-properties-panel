{
  "name": "dmn-js-properties-panel",
  "version": "3.4.1",
  "description": "A properties panel for dmn-js",
  "files": [
    "dist"
  ],
  "scripts": {
    "all": "run-s lint test distro",
    "distro": "run-s build test:build",
    "build": "run-p bundle",
    "bundle": "rollup -c --bundleConfigAsCjs",
    "bundle:watch": "npm run bundle -- -w",
    "lint": "eslint .",
    "dev": "npm test -- --auto-watch --no-single-run",
    "test": "karma start karma.config.js",
    "test:build": "mocha --reporter=spec --recursive test/distro",
    "start": "cross-env SINGLE_START=platform npm run dev",
    "start:platform": "cross-env SINGLE_START=platform npm run dev",
    "start:dmn": "cross-env SINGLE_START=dmn npm run dev",
    "prepare": "run-s bundle"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bpmn-io/dmn-js-properties-panel"
  },
  "keywords": [
    "bpmn-io",
    "dmn-js",
    "properties",
    "properties-panel"
  ],
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "author": {
    "name": "Nico Rehwaldt",
    "url": "https://github.com/nikku"
  },
  "license": "MIT",
  "engines": {
    "node": "*"
  },
  "dependencies": {
    "diagram-js": "^14.3.1",
    "min-dash": "^4.2.1",
    "min-dom": "^4.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.24.7",
    "@babel/plugin-transform-react-jsx": "^7.24.7",
    "@bpmn-io/properties-panel": "^3.22.0",
    "@rollup/plugin-alias": "^5.1.0",
    "@rollup/plugin-babel": "^6.0.3",
    "@rollup/plugin-commonjs": "^26.0.0",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.1.0",
    "@svgr/rollup": "^8.1.0",
    "@svgr/webpack": "^8.1.0",
    "@testing-library/preact": "^3.2.4",
    "babel-loader": "^9.1.2",
    "babel-plugin-istanbul": "^7.0.0",
    "camunda-dmn-moddle": "^1.3.0",
    "chai": "^4.4.1",
    "cross-env": "^7.0.3",
    "dmn-js": "^16.4.0",
    "dmn-js-drd": "^16.4.0",
    "dmn-js-shared": "^16.4.0",
    "eslint": "^8.57.0",
    "eslint-plugin-bpmn-io": "^1.0.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-react-hooks": "^4.6.2",
    "karma": "^6.4.3",
    "karma-chrome-launcher": "^3.2.0",
    "karma-coverage": "^2.2.1",
    "karma-debug-launcher": "^0.0.5",
    "karma-env-preprocessor": "^0.1.1",
    "karma-mocha": "^2.0.1",
    "karma-sinon-chai": "^2.0.2",
    "karma-webpack": "^5.0.1",
    "mocha": "^10.4.0",
    "mocha-test-container-support": "^0.2.0",
    "npm-run-all2": "^6.2.0",
    "puppeteer": "^22.11.2",
    "rollup": "^4.18.0",
    "rollup-plugin-copy": "^3.4.0",
    "sinon": "^17.0.1",
    "sinon-chai": "^3.7.0",
    "webpack": "^5.92.1"
  },
  "peerDependencies": {
    "@bpmn-io/properties-panel": ">=3.7",
    "dmn-js": ">=13"
  }
}
